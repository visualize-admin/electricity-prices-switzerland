    "@mdx-js/react": "^1.6.22",
    "@next/bundle-analyzer": "^10.2.0",
    "@next/mdx": "^10.2.0",
    "@rdfjs/namespace": "^1.1.0",
    "@reach/dialog": "^0.15.0",
    "@reach/visually-hidden": "^0.15.0",
    "@tpluscode/rdf-ns-builders": "^0.4.2",
    "@tpluscode/rdf-string": "^0.2.23",
    "@tpluscode/sparql-builder": "^0.3.12",
    "apollo-server-micro": "^2.23.0",
    "catalog": "^4.0.1-canary.2",
    "cors": "^2.8.5",
    "d3": "^6.7.0",
    "downshift": "^6.1.3",
    "fp-ts": "^2.10.4",
    "fs-extra": "^9.1.0",
    "global-agent": "^2.2.0",
    "graphql": "^15.5.0",
    "graphql-parse-resolve-info": "^4.10.0",
    "graphql-tag": "^2.11.0",
    "graphql-type-json": "^0.3.2",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "make-plural": "^6.2.2",
